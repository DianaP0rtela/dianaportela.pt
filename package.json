{
  "name": "promatik-pt",
  "version": "0.0.1",
  "description": "Development project for promatik.pt",
  "main": "index.html",
  "scripts": {
    "start": "npm run clear-cache && parcel src/index.html --open",
    "build": "npm run clear-cache && parcel build src/index.html --no-source-maps && npm run copy-public",
    "clear-cache": "del-cli --force ./.cache && del-cli --force ./dist",
    "copy-public": "cp -r ./src/public/* ./dist"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "del-cli": "^3.0.1",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1",
    "normalize.css": "8.0.1",
    "parcel-bundler": "^1.12.4",
    "posthtml": "^0.13.4",
    "posthtml-expressions": "^1.5.0",
    "posthtml-include": "^1.5.0",
    "sass": "^1.27.0"
  },
  "keywords": [],
  "dependencies": {}
}
